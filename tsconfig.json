{
  "include": [
    "src/**/*.ts"
  ],
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext","dom"],
    "moduleResolution": "node",
    "types": ["lua-types/5.2","@typescript-to-lua/language-extensions","wireshark-lua-api-declarations"],
    "outDir": "./dist",
    "removeComments": false,
    "strict": true

  },
  "tstl": {
    "luaTarget": "5.2",
    "luaLibImport": "inline"
  }

}
